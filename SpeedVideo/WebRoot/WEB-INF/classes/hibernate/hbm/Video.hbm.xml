<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ywh.entity.Video" table="t_video">
		<id name="id">
			<generator class="native"></generator>
		</id>
		<property name="title"></property>
		<property name="intro"></property>
		<property name="tag"></property>
		<property name="url"></property>
		<property name="image"></property>
		<property name="views" type="int"></property>
		<property name="cid"></property>
		<property name="upload_date"></property>
	</class>
</hibernate-mapping>
