<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ywh.entity.Comment" table="t_comment">
		<id name="id">
			<generator class="native"></generator>
		</id>
		<property name="vid"></property>
		<property name="uid"></property>
		<!--
			<one-to-one name="video" class="com.ywh.entity.Video"
			constrained="true" property-ref="id"></one-to-one> <one-to-one
			name="user" class="com.ywh.entity.User" constrained="true"
			property-ref="id"></one-to-one>
		-->
		<property name="row"></property>
		<property name="content"></property>
		<property name="postdate"></property>
	</class>
</hibernate-mapping>
